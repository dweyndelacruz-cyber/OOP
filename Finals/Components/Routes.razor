@using Microsoft.AspNetCore.Components
@using Microsoft.AspNetCore.Components.Routing

<Router AppAssembly="@typeof(Finals.App).Assembly">
  <Found Context="routeData">
    <RouteView RouteData="routeData" DefaultLayout="@typeof(Finals.MainLayout)" />
  </Found>
  <NotFound>
    <LayoutView Layout="@typeof(Finals.MainLayout)">
      <p>Sorry, there's nothing at this address.</p>
    </LayoutView>
  </NotFound>
</Router>